a = input("Masukkan bilangan pertama: ")
b = input("Masukkan bilangan kedua: ")
c = input("Masukkan bilangan ketiga: ")

try:
    a = int(a)
    b = int(b)
    c = int(c)

    if a > b and a > c:
        print("Bilangan terbesar:", a)
    elif b > a and b > c:
        print("Bilangan terbesar:", b)
    else:
        print("Bilangan terbesar:", c)
except ValueError:
    print("angka tidak valdi !! masukkan angka interger!!")
