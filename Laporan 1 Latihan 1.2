harga_beli_per_gram = 650000
jumlah_gram = 25
harga_jual_per_gram = 685000

#harga beli
harga_beli = harga_beli_per_gram * jumlah_gram

#harga jual
harga_jual = harga_jual_per_gram * jumlah_gram

#keuntungan rupiah
keuntungan_rupiah = harga_jual - harga_beli

#keuntungan persen
keuntungan_persen = (keuntungan_rupiah / harga_beli) * 100

print ("KEUNTUNGAN:" ,keuntungan_rupiah, keuntungan_persen)
